<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextInputLayout">
        <attr name="title" format="string"/>
        <attr name="hint" format="string"/>
        <attr name="inputType" format="string"/>
        <attr name="buttonText" format="string"/>
    </declare-styleable>

    <declare-styleable name="LoginTopBar">
        <attr name="login_top_bar_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="RegisterTopBar">
        <attr name="phase" format="integer"/>
    </declare-styleable>

    <declare-styleable name="MyFundCard">
        <attr name="my_fund_type" format="integer"/>
    </declare-styleable>

    <declare-styleable name="StepButton">
        <attr name="step" format="integer"/>
    </declare-styleable>

    <declare-styleable name="CustomTextInputLayoutPresentBottomSheet">
        <attr name="text_field_title" format="string"/>
        <attr name="text_field_hint" format="string"/>
    </declare-styleable>

</resources>